.switch {
  display: inline;
  left: 0;
  position: relative;
  width: 70%; 
}
.switch input[type="checkbox"] {
  position: absolute;
  visibility: hidden; 
}
.switch input[type="checkbox"] + label::before {
  background-color: #fff;
  border-radius: 60px;
  right: 1px;
  transition: background 0.4s ease 0s;
}
.switch input[type="checkbox"] + label::before, .switch input[type="checkbox"] + label::after {
  bottom: 1px;
  content: "";
  display: block;
  left: 1px;
  position: absolute;
  top: 1px;
}
.switch input[type="checkbox"] + label::after {
  background-color: #ffffff;
  border-radius: 100%;
  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3);
  transition: margin 0.4s ease 0s;
  width: 22px;
}
.switch {
	left: 0;
	position: relative;
	top: 6px;
	width: 70%;
}
.switch input[type="checkbox"] {
	position: absolute;
	margin-left: -9999px;
	visibility: hidden;
}
.switch + input[type="checkbox"] + label {
	display: block;
	position: relative;
	cursor: pointer;
	outline: none;
	user-select: none; 
}
.switch input[type="checkbox"] + label {
	background-color: #ffffff;
	border-radius: 60px;
	height: 25px;
	padding: 2px;
	width: 43px;
	border: 1px solid #ddd; cursor:pointer;
}
.switch input[type="checkbox"]:checked + label {
	border: none;
}
.switch input[type="checkbox"] + label::before, .switch input[type="checkbox"] + label::after {
  bottom: 11px;
  content: "";
  display: block;
  left: 1px;
  position: absolute;
  top: -14px;
}
.switch input[type="checkbox"] + label:before {
	right: 1px;
	background-color: #fff;
	border-radius: 60px;
	transition: background 0.4s;
}
.switch input[type="checkbox"] + label:after {
	background-color: #ffffff;
	border-radius: 100%;
	box-shadow: 0 2px 5px rgba(0, 0, 0, 0.3);
	transition: margin 0.4s ease 0s;
	width: 22px; 
}
.switch input[type="checkbox"]:checked + label:before {
	background-color: #04656e; top:-16px;
}
.switch input[type="checkbox"]:checked + label:after {
	margin-left:20px; margin-top:-2px;
}




.ryt-chk input[type="checkbox"] {  display: none;}
.ryt-chk input[type="checkbox"] ~ label {  cursor: pointer;  font-weight: normal;  margin-bottom: 10px;  margin-left: 16px;  padding-left: 10px;}
.ryt-chk input[type="checkbox"] ~ label::before {  content: url("../images/uncheck.png");  margin-left: -25px;  padding-right: 11px;}
.ryt-chk input[type="checkbox"]:checked ~ label::before {  content: url("../images/check.png");}

.ryt-chk input[type="checkbox"] ~ label::after {  content: url("../images/check.png"); display:none;}



