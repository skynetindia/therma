.ribbon1 span {
  background: #00abbb none repeat scroll 0 0;
  border-top-right-radius: 8px;
  color: #fff;
  display: block;
  font-size: 14px;
  line-height: 1;
  padding: 12px 8px 4px;
  position: relative;
  text-align: center;
  width: 105px; text-transform:capitalize; font-weight:bold;
}
.ribbon1::after {
  border-left: 53px solid transparent;
  border-right: 53px solid transparent;
  border-top: 10px solid #00ABBB;
  content: "";
  height: 0;
  position: absolute;
  width: 0;
}
.ribbon1 span::before {
  background: #00ABBB none repeat scroll 0 0;
  height: 6px;
  left: -6px;
  top: 0;
  width: 6px;
}
.ribbon1 span::before, .ribbon1 span::after {
  content: "";
  position: absolute;
}

.ribbon1 span::after {
  background:#00626b none repeat scroll 0 0;
  border-radius: 8px 8px 0 0;
  height: 6px;
  left: -8px;
  top: 0;
  width: 8px;
}

.ribbon1 span::before, .ribbon1 span::after {
  content: "";
  position: absolute;
}

